canvas {
  position: fixed;
  top: 0;
  left: 400px;
  width: 1125px;
  height: 1125px;
}

.tag {
  width: 200px;
  height: 200px;
  position: absolute;
  top: 80px;
  left: 25%;
  z-index: 3;
  border: 1px dashed #fff;
}

.preview {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.9);
  position: absolute;
  z-index: 2;
}

.bottomAll {
  width: 100%;
  position: absolute;
  bottom: 5px;
  z-index: 3;
}
.btnAll{
  width: 100%;
  display: flex;
  flex-direction: row;
}
.item {
  width: 25%;
  display: flex;
  flex-direction: column;
}
.item span {
  font-size: 14px;
}
.demo {
  width: 100%;
  height: 60px;
}

.btn {
  width: 100%;
  font-size: 0.6rem;
}
.save{
  padding: 20px 0px 10px 0px;
}
