<view class="messages">
  <!-- 秒杀倒计时 -->
  <view class="miaosha">
    <view>电商秒杀倒计时：</view>
    <text>秒杀: {{clock}}</text>
    <text>{{micro_second}}</text>
  </view>
  <!-- 动态轮播图 -->
  <swiper indicator-dots="true" autoplay="true" indicator-color="yellow">
    <block wx:for="{{imgUrls}}" wx:key="item">
      <swiper-item>
        <image src="{{item}}" />
      </swiper-item>
    </block>
  </swiper>
  <button bindtap='getImages' type='primary' class='btn'>云开发动态替换图片</button>

  <view class="videos">
    <button type='primary' bindtap="videos">视频播放器</button>
  </view>
</view>
